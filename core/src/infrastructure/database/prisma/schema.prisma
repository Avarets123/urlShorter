generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "linux-musl", "linux-musl-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model UrlModel {
  id          String @unique @default(uuid()) @db.Uuid
  originalUrl String @unique
  newUrl      String @unique
}
